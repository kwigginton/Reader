<h1>Listing feeds</h1>

<table>
  <tr>
    <th> url</th>
    <th>Title</th>
    <th>Author</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feeds.each do |feed| %>
  <tr>
    <td><%= feed. url %></td>
    <td><%= feed.title %></td>
    <td><%= feed.author %></td>
    <td><%= link_to 'Show', feed %></td>
    <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
    <td><%= link_to 'Destroy', feed, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

